.body {
    min-height: 100vh;
    padding-top: 60rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.logo-label {
    width: 228rpx;
    height: 216rpx;
    margin-bottom: 20rpx;
}

.title {
    height: 56rpx;
    font-size: 40rpx;
    font-family: PingFangSC-Medium;
    font-weight: bolder;
    color: rgba(0,0,0,0.8);
    line-height: 56rpx;
}

.des {
    height: 32rpx;
    font-size: 22rpx;
    font-weight: 400;
    color: rgba(153,153,153,1);
    line-height: 32rpx;
}

.money {
    height: 40rpx;
    font-size: 28rpx;
    font-weight: bolder;
    color: rgba(102,102,102,1);
    line-height: 40rpx;
    margin: 10rpx 0 40rpx;
}

.navs {
    display: flex;
    width: 470rpx;
    height: 80rpx;
    justify-content: center;
    padding-bottom: 70rpx;
}

.nav-item {
    width: 230rpx;
    height: 80rpx;
    border-radius: 5rpx;
    border: 1rpx solid rgba(193,193,193,1);
    display: flex;
    justify-content: center;
    align-items: center;
    color: #999;
}

.coupon {
    background: linear-gradient(135deg,rgba(5,178,155,1) 0%,rgba(59,233,178,1) 100%);
    border: none;
    color: #fff;
    margin-left: 10rpx;
}

.goods-list {
    padding: 0 25rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.goods-item {
    width: 340rpx;
    height: 400rpx;
    border: 1rpx solid #e6e6e6;
    border-radius: 10rpx;
    position: relative;
    flex: 0;
    margin-bottom: 24rpx;
    background: #fff;
}

.goods-item .img-box {
    width: 340rpx;
    height: 280rpx;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    overflow: hidden;
}

.goods-item .img-box image {
    width: 340rpx;
    height: 280rpx;
}

.goods-item .img-box .group-icon {
    background: #3cdbc0;
    font-size: 26rpx;
    color: #fff;
    height: 40rpx;
    border-top-left-radius: 10rpx;
    border-bottom-right-radius: 10rpx;
    padding: 0 12rpx 0 14rpx;
    position: absolute;
    left: 0;
    top: 0;
}

.goods-info {
    width: 100%;
}

.goods-name {
    font-size: 30rpx;
    line-height: 48rpx;
    color: #333;
    padding: 0 16rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: block;
}

.goods-price {
    font-size: 20rpx;
    color: #494949;
    line-height: 44rpx;
    margin-left: 16rpx;
    margin-top: 11rpx;
}

.sell-price {
    font-size: 32rpx;
    color: #f98f68;
    margin-right: 7rpx;
}

.old-price {
    text-decoration: line-through;
}

.button-box {
    width: 120rpx;
    height: 44rpx;
    background: #3cdbc0;
    box-shadow: 0rpx 4rpx 10rpx 0rpx rgba(0,159,85,0.2);
    border-radius: 4rpx;
    font-size: 24rpx;
    color: #fff;
    font-weight: bold;
    text-align: center;
    line-height: 44rpx;
    position: absolute;
    bottom: 12rpx;
    right: 21rpx;
}

.page-content {
    height: 100%;
    width: 100%;
    overflow: scroll;
    -webkit-overflow-scrolling: touch;
}

movable-area {
    height: 100%;
    width: 100%;
}

.movable-view {
    width: 120rpx;
    height: 120rpx;
}

movable-view {
    width: 120rpx;
    height: 120rpx;
}

.popup-box {
    width: 562rpx;
    min-height: 504rpx;
    background: #fff;
    border-radius: 10rpx;
    position: relative;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
}

.popup-close {
    width: 44rpx;
    height: 44rpx;
    position: absolute;
    right: 19rpx;
    top: 19rpx;
}

.popup-close image {
    width: 22rpx;
    height: 22rpx;
}

.popup-title {
    color: #1c1c1c;
    font-size: 36rpx;
    font-weight: 400;
    line-height: 60rpx;
    margin-top: 44rpx;
}

.popup-content {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: flex-start;
    padding: 59rpx 0;
}

.popup-content text {
    display: block;
}

.popup-text {
    color: #3cdbc0;
    font-size: 44rpx;
    line-height: 68rpx;
    font-weight: 500;
    text-align: center;
    width: 374rpx;
}

.popup-text-end {
    color: #3cdbc0;
    font-size: 44rpx;
    line-height: 56rpx;
    font-weight: bold;
    text-align: center;
    width: 374rpx;
}

.popup-tips {
    color: #1c1c1c;
    font-size: 36rpx;
    font-weight: 400;
    line-height: 54rpx;
}

.popup-button {
    padding-bottom: 52rpx;
    text-align: center;
}

.popup-button text {
    font-size: 30rpx;
    line-height: 50rpx;
    color: #999;
    margin-bottom: 10rpx;
    display: block;
}

.share-btn {
    font-size: 30rpx;
    color: #fff;
    font-weight: 400;
    text-align: center;
    width: 369rpx;
    height: 70rpx;
    line-height: 70rpx;
    background: #3cdbc0;
    border-radius: 10rpx;
    padding: 0;
}

.popup-luckdraw-box {
    padding-bottom: 20rpx;
    position: relative;
}

.popup-luckdraw-close {
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    right: 66rpx;
    top: 80rpx;
}

.popup-luckdraw-close image {
    width: 40rpx;
    height: 40rpx;
}

.more-title {
    background: rgba(245,245,245,1);
}

.recommending-title {
    padding: 20rpx 0 0rpx;
}

.money-box {
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 80rpx;
    padding: 0 140rpx;
}

.money-box .des-txt {
    margin: 0 20rpx;
    font-weight: bolder;
    height: 28rpx;
    font-size: 20rpx;
    font-family: PingFangSC-Medium;
    color: rgba(0,0,0,0.3);
    line-height: 28rpx;
}

.money-box .line {
    width: 100rpx;
    height: 3rpx;
    flex: 1;
}

.money-box .line-left {
    background: #CCCCCC;
}

.money-box .line-right {
    background: #CCCCCC;
}
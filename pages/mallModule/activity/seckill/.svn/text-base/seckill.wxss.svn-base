view.unable {
    background: url('https://xianfengapp.oss-cn-hangzhou.aliyuncs.com/xianfengImages/sprite3/skill_none.png') center center no-repeat;
    background-size: 170rpx 56rpx;
}

.goods-info button::after {
    border: none;
}

.goods-info button.unable {
}

.goods-info button.enable {
}

.progress-box {
    position: relative;
    height: 28rpx;
    display: flex;
    margin-top: 6rpx;
    align-items: center;
    overflow: hidden;
}

.progress-box text {
    font-size: 20rpx;
    font-family: PingFangSC-Regular;
    color: rgba(205,159,100,1);
    line-height: 28rpx;
    margin-left: 12rpx;
}

.progress-box progress {
    width: 200rpx;
    height: 8px;
    border-radius: 4px;
    border: 1px solid rgba(255,221,177,1);
    overflow: hidden;
}

.no-data {
    width: 100%;
    height: 100%;
    text-align: center;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    background: #fff;
}

.goods-content .no-data {
}

.time-slot {
    height: auto;
    margin-top: 30%;
}

.no-data image {
    width: 242rpx;
    height: 261rpx;
}

.no-data button {
    color: #3CDBC0;
    border: 1rpx solid #3CDBC0;
    margin-top: 50rpx;
    width: 202rpx;
    height: 60rpx;
    background: #fff;
}

movable-area {
    height: 100%;
    width: 100%;
}

.movable-view {
    width: 120rpx;
    height: 120rpx;
}

movable-view {
    width: 120rpx;
    height: 120rpx;
}
.content {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.prompt-box {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 74rpx;
}

.success-icon {
    width: 109rpx;
    height: 109rpx;
}

.success-text {
    font-size: 38rpx;
    line-height: 38rpx;
    color: #313131;
    margin-top: 28rpx;
}

.btn-box {
    display: flex;
    flex-direction: row;
    padding-bottom: 61rpx;
}

.btn-box .btn {
    margin: 0 21rpx;
    width: 193rpx;
    height: 69rpx;
    background: #ffffff;
    border:2rpx solid rgba(201,201,201,1);
    border-radius:6rpx;
    font-size: 26rpx;
    line-height: 69rpx;
    text-align: center;
    color: #737373;
    box-sizing: border-box;
}
.btn-box .btn:nth-child(2){
  background-color: #FC7176;
  color: #fff;
  border: none;
}
button::after {
    border: none;
}

.delivery-box {
    background: #ffffff;
    margin-bottom: 20rpx;
    padding: 14rpx 0 10rpx 0;
    text-align: center;
    margin-top: 40rpx;
    display: flex;
    flex-direction: column;
}

.delivery-box .title {
    color: #646464;
    font-size: 32rpx;
    line-height: 56rpx;
    margin-bottom: 20rpx;
}

.code-box {
    width: 100%;
}

.code-box .goods-code {
    width: 535rpx;
    height: 130rpx;
}

.barcode-box {
    width: 535rpx;
    height: 130rpx;
    margin: 0 auto;
}

#barcode {
    width: 535rpx;
    height: 130rpx;
    margin: 0 auto;
}

.code-box text {
    width: 100%;
    display: block;
    font-size: 48rpx;
    line-height: 68rpx;
    margin-bottom: 2rpx;
}

.tips {
    width: 600rpx;
    font-size: 32rpx;
    color: #606060;
    line-height: 56rpx;
    display: block;
}

.recommending-box {
    width: 100%;
    background: #e9e9e9;
}

.recommending-title {
    font-size: 34rpx;
    line-height: 64rpx;
    position: relative;
    text-align: center;
    margin:35rpx 0;
}

.recommending-title::before {
    left: 89rpx;
    top: 37rpx;
}

.recommending-title::after {
    right: 89rpx;
    top: 37rpx;
}

.goods-list {
    padding: 0 30rpx;
    width: 100%;
    box-sizing: border-box;
}

.goods-item {
    height: 520rpx;
    border: 1rpx solid #E6E6E6;
    width: 336rpx;
    border-radius: 10rpx;
    position: relative;
    margin-bottom: 20rpx;
    background: #ffffff;
    box-shadow:0px 2px 8px 0px rgba(217,217,217,0.75);
    margin-right: 19rpx;
    float: left;
    box-sizing: border-box;
}
.goods-item:nth-child(2n){
  margin-right: 0;
}
.goods-item .img-box {
    width: 323rpx;
    height: 304rpx;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    overflow: hidden;
}

.goods-item .img-box image {
    width: 340rpx;
    height: 280rpx;
}

.goods-item .img-box .group-icon {
    background: #FE2A36;
    font-size: 26rpx;
    color: #ffffff;
    height: 40rpx;
    border-top-left-radius: 10rpx;
    border-bottom-right-radius: 10rpx;
    padding: 0 12rpx 0 14rpx;
    position: absolute;
    left: 0;
    top: 0;
}

.goods-info {
    width: 100%;
}

.goods-name {
    font-size:28rpx;
    font-family:FZLanTingHei-R-GBK;
    font-weight:400;
    color:rgba(44,43,43,1);
    line-height:38rpx;
    padding: 0 16rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: block;
}
.goods-discription{
  font-size:22rpx;
  font-family:FZLanTingHeiS-R-GB;
  font-weight:400;
  color:rgba(161,161,161,1);
  line-height:52rpx;
  padding: 0 16rpx;
  display: block;
  overflow: hidden;
  margin-top: 14rpx;
  height:52rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.goods-price {
    font-size: 20rpx;
    color: #494949;
    line-height: 49rpx;
    margin-left: 16rpx;
    margin-top: 18rpx;
    position: relative;
}

.sell-price {
    font-size: 32rpx;
    color: #FE2A36;
    margin-right: 7rpx;
    line-height: 49rpx;
    font-weight:bold;
}

.old-price {
    text-decoration: line-through;
}
.sell-price text{
  font-size: 21rpx;
  font-weight:bold;
}
.button-box {
    width: 113rpx;
    height: 41rpx;
    background:linear-gradient(90deg,rgba(254,94,103,1),rgba(254,42,54,1));
    border-radius: 41rpx;
    font-size: 20rpx;
    color: #ffffff;
    font-weight: bold;
    text-align: center;
    line-height: 39rpx;
    position: absolute;
    bottom:34rpx;
    right: 21rpx;
    top:0;
}

.popup-box {
    width: 562rpx;
    min-height: 504rpx;
    background: #ffffff;
    border-radius: 10rpx;
    position: relative;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
}

.popup-close {
    width: 44rpx;
    height: 44rpx;
    position: absolute;
    right: 19rpx;
    top: 19rpx;
}

.popup-close image {
    width: 22rpx;
    height: 22rpx;
}

.popup-title {
    color: #1C1C1C;
    font-size: 36rpx;
    font-weight: 400;
    line-height: 60rpx;
    margin-top: 44rpx;
}

.popup-content {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: flex-start;
    padding: 59rpx 0;
}

.popup-content text {
    display: block;
}

.popup-text {
    color: #019F55;
    font-size: 44rpx;
    line-height: 68rpx;
    font-weight: 500;
    text-align: center;
    width: 374rpx;
}

.popup-text-end {
    color: #019F55;
    font-size: 44rpx;
    line-height: 56rpx;
    font-weight: bold;
    text-align: center;
    width: 374rpx;
}

.popup-tips {
    color: #1C1C1C;
    font-size: 36rpx;
    font-weight: 400;
    line-height: 54rpx;
}

.popup-button {
    padding-bottom: 52rpx;
    text-align: center;
}

.popup-button text {
    font-size: 30rpx;
    line-height: 50rpx;
    color: #999999;
    margin-bottom: 10rpx;
    display: block;
}

.share-btn {
    font-size: 30rpx;
    color: #ffffff;
    font-weight: 400;
    text-align: center;
    width: 369rpx;
    height: 70rpx;
    line-height: 70rpx;
    background: #019F55;
    border-radius: 10rpx;
    padding: 0;
}

.popup-luckdraw-box {
    padding-bottom: 20rpx;
    position: relative;
}

.popup-luckdraw-close {
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    right: 66rpx;
    top: 80rpx;
}

.popup-luckdraw-close image {
    width: 40rpx;
    height: 40rpx;
}
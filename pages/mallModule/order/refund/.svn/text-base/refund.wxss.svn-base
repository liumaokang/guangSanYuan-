page {
    background-color: #fff;
}

.refund-main {
    width: 100%;
    padding-bottom: 200rpx;
}

.refund-order {
    width: 100%;
    background-color: #fff;
    border-top: 1rpx solid #e6e6e6;
    border-bottom: 10rpx solid#F1F1F1;
    color: #454545;
    padding-bottom: 35rpx;
}

.order-number {
    height: 80rpx;
    font-size: 24rpx;
    line-height: 80rpx;
    border-bottom: 1px solid #ebebeb;
    margin: 0 40rpx;
    color: #505050;
    display: flex;
    align-items: center;
}

.order-number checkbox {
    line-height: 80rpx;
}

checkbox .wx-checkbox-input {
    border-radius: 50%;
    width: 32rpx;
    height: 32rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
    border-color: #009136;
    background: #009136;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
    border-radius: 50%;
    width: 32rpx;
    height: 32rpx;
    line-height: 32rpx;
    text-align: center;
    font-size: 20rpx;
    color: #fff;
    background: transparent;
    transform: translate(-50%,-50%) scale(1);
}

.goods-box {
    display: flex;
    flex-wrap: wrap;
    padding: 10rpx 40rpx 10rpx 40rpx;
}

.goods-box checkbox-group {
    width: 100%;
}

.goods-box.portion-refund .goods-item {
    width: 100%;
    height: 120rpx;
    margin: 10rpx 0;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    border: none;
}

.goods-box.portion-refund .goods-info {
    position: relative;
    width: calc(100% - 150rpx);
    font-size: 32rpx;
    height: 120rpx;
}

.goods-box .goods-item {
    width: 120rpx;
    height: 120rpx;
    margin: 0 8rpx 9rpx 0;
    border: 1rpx solid #f0f0f0;
}

.goods-box .goods-item:last-of-type {
    margin-right: 0;
}

.goods-box .goods-item>image {
    width: 120rpx;
    height: 120rpx;
}

.goods-box .goods-item checkbox {
    height: 120rpx;
    line-height: 120rpx;
}

.goods-box .goods-name,.goods-box .goods-desc {
    display: block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.goods-box .goods-name {
    font-size: 32rpx;
    line-height: 36rpx;
    height: 36rpx;
    margin-top: 12rpx;
    margin-bottom: 32rpx;
}

.goods-box .goods-desc {
    font-size: 26rpx;
    line-height: 36rpx;
    width: 70%;
}

.goods-box .price-number .count-box {
    width: 135rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    line-height: 44rpx;
    height: 44rpx;
    border: 1rpx solid #f1f1f1;
    position: absolute;
    bottom: 0;
    right: 0;
}

.goods-box .price-number .count-box text {
    background: #fff;
    color: #454545;
    font-size: 24rpx;
    width: 43rpx;
    height: 100%;
    text-align: center;
}

.goods-box .price-number .count-box view {
    width: 46rpx;
    height: 46rpx;
    text-align: center;
    font-size: 40rpx;
    background: #f1f1f1;
    color: #009136;
    display: flex;
    align-items: center;
    justify-content: center;
}

.order-info {
    font-size: 24rpx;
    height: 36rpx;
    line-height: 36rpx;
    width: 100%;
    margin-top: 26rpx;
    padding: 0 40rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: flex-end;
    align-items: baseline;
}

.order-info>view {
    display: inline-flex;
}

.order-info>view:last-of-type view {
    vertical-align: bottom;
}

.goods-count {
    margin-right: 22rpx;
}

.order-price {
    color: #F64145;
    font-size: 24rpx;
}

.freight-info {
    font-size: 26rpx;
    margin-left: 15rpx;
    line-height: 26rpx;
    margin-top: 10rpx;
}

.order-freight {
    font-size: 26rpx;
    color: #727272;
}

.refund-box {
    background-color: #fff;
    padding: 0 40rpx 9rpx 40rpx;
    width: 100%;
    box-sizing: border-box;
}

.refund-title {
    padding-top: 12rpx;
    font-size: 26rpx;
    font-weight: bold;
    line-height: 38rpx;
    color: #2C2B2B;
    padding-bottom: 22rpx;
    border-bottom: 1rpx solid #f1f1f1;
}

.redund-explain {
    padding: 0;
}

.redund-labels {
    padding: 20rpx 0;
}

.redund-labels text {
    font-size: 24rpx;
    line-height: 60rpx;
    height: 60rpx;
    color: #505050;
    box-sizing: border-box;
    padding: 0 26rpx;
    display: inline-block;
    border-radius: 4rpx;
    margin: 10rpx 20rpx 10rpx 0;
    background:rgba(246,246,246,1);
}

.redund-labels text.select {
    background: #F89093;
    color: #fff;
}

.redund-explain-con {
    width: 598rpx;
    height: 192rpx;
    font-size: 28rpx;
    line-height: 32rpx;
    min-height: 192rpx;
    color: #8b8b8c;
    padding: 26rpx 36rpx;
    background:rgba(238,238,238,1);
    border-radius: 10rpx;
}

.refund-image {
    display: flex;
    flex-wrap: wrap;
    margin-top: 20rpx;
}

.refund-image>image {
    width: 162rpx;
    height: 162rpx;
    margin-right: 5rpx;
    margin-bottom: 9rpx;
}

.refund-image>image:last-of-type {
    margin-right: 0;
}

.redund-explain .submit-img {
    width: 162rpx;
    height: 162rpx;
}

.bottom-box {
    position: fixed;
    bottom: 0;
    width: 100%;
    padding: 0 40rpx;
    box-sizing: border-box;
    height: 120rpx;
    border-top: 1rpx solid #dcdcdc;
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: #fff;
    z-index: 9;
}

.bottom-box .order-price {
    color: #F64145;
    font-size: 36rpx;
    line-height: 120rpx;
    font-weight: bold;
}

.bottom-box .order-price text {
    font-size: 24rpx;
    color: #6F6F6F;
    font-weight: bold;
    line-height: 30rpx;
    padding-left: 22rpx;
    margin-left: 22rpx;
    border-left: 1px solid rgba(147,147,147,1);
}

.btn {
    font-size: 29rpx;
    line-height: 64rpx;
    font-weight: bold;
    height: 64rpx;
    width: 203rpx;
    color: #fff;
    border-radius: 64rpx;
    border: 0;
    margin: 0;
    background:linear-gradient(90deg,rgba(255,96,96,1),rgba(246,65,69,1));
}

.refund-success {
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.success-img {
    width: 240rpx;
    height: 227rpx;
    margin-top: 280rpx;
}

.success-tips {
    font-size: 32rpx;
    line-height: 92rpx;
    color: #8b8b8c;
}

.retuen-btn {
    font-size: 32rpx;
    font-weight: bold;
    color: #fff;
    width: 325rpx;
    height: 100rpx;
    line-height: 100rpx;
    background: #3cdbc0;
    border-radius: 6rpx;
}

.popup-luckdraw-box {
    width: 590rpx;
    height: 380rpx;
    background: rgba(255,255,255,1);
    border-radius: 10rpx;
    overflow: hidden;
    text-align: center;
    display: flex;
    flex-direction: column;
}

.popup-luckdraw-box .pop-title {
    height: 50rpx;
    font-size: 36rpx;
    padding: 70rpx 0 10px;
    font-family: PingFangSC-Medium;
    font-weight: bolder;
    color: rgba(0,0,0,0.8);
    line-height: 50rpx;
}

.popup-luckdraw-box .pop-content {
    font-size: 26rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(102,102,102,1);
    line-height: 36rpx;
    width: 416rpx;
    margin: 0 auto;
    flex: 1;
}

.popup-luckdraw-box .pop-btn {
    display: flex;
    justify-content: space-between;
    height: 100rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(102,102,102,1);
    background: rgba(245,245,245,1);
}

.pop-btn text {
    flex: 1;
    display: flex;
    justify-content: center;
    align-items: center;
}

.pop-btn text.btn-sure {
    background: linear-gradient(270deg,rgba(60,219,192,1) 0%,rgba(66,229,172,1) 100%);
    color: #ffffff;
}